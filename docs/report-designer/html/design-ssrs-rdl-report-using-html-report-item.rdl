                <Name>DocumentValue</Name>
                <Value>&lt;style&gt;
        .pie-chart {
            width: 150px;
            height: 150px;
            border-radius: 50%;
            background: conic-gradient(
                green 0% 40%,
                dodgerblue 40% 70%,
                orange 70% 90%,
                red 90% 100%
            );
            margin: 50px auto;
        }

        .legend {
            width: fit-content;
            margin: auto;
            margin-top: 20px;
        }

        .legend-item {
            display: flex;
            align-items: center;
            margin-bottom: 5px;
        }

        .color-box {
            width: 15px;
            height: 15px;
            margin-right: 10px;
        }

        .green { background-color: green; }
        .blue { background-color: dodgerblue; }
        .orange { background-color: orange; }
        .red { background-color: red; }
    &lt;/style&gt;
&lt;body&gt;

    &lt;div class="pie-chart"&gt;&lt;/div&gt;

    &lt;div class="legend"&gt;
        &lt;div class="legend-item"&gt;&lt;div class="color-box green"&gt;&lt;/div&gt;Product A - 40%&lt;/div&gt;
        &lt;div class="legend-item"&gt;&lt;div class="color-box blue"&gt;&lt;/div&gt;Product B - 30%&lt;/div&gt;
        &lt;div class="legend-item"&gt;&lt;div class="color-box orange"&gt;&lt;/div&gt;Product C - 20%&lt;/div&gt;
        &lt;div class="legend-item"&gt;&lt;div class="color-box red"&gt;&lt;/div&gt;Product D - 10%&lt;/div&gt;
    &lt;/div&gt;

&lt;/body&gt;</Value>
