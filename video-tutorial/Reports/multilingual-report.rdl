  <DataSets>
    <DataSet Name="SalesOrderDataset">
      <Query>
        <DataSourceName>AdventureWorks</DataSourceName>
        <CommandText>SELECT 
  [Sales].[SalesOrderDetail].[SalesOrderID] AS 'Sales ID',
  SUM([UnitPrice]) AS total_Unit_Price,
  SUM([OrderQty]) AS total_Order_Quantity
FROM [Sales].[SalesOrderDetail]
GROUP BY [SalesOrderID]</CommandText>
      </Query>
      <Fields>
        <Field Name="Sales_ID">
          <DataField>Sales ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="total_Unit_Price">
          <DataField>total_Unit_Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="total_Order_Quantity">
          <DataField>total_Order_Quantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="LocalizedTextDataset">
      <Query>
        <DataSourceName>AdventureWorks</DataSourceName>
        <CommandText>SELECT [dbo].[LocalizedText].[LanguageCode],
[dbo].[LocalizedText].[LocalizedTextKey],
[dbo].[LocalizedText].[LocalizedText] FROM [dbo].[LocalizedText]
WHERE LanguageCode=@Language</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Language">
            <Value>=Parameters!Language.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="LanguageCode">
          <DataField>LanguageCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LocalizedTextKey">
          <DataField>LocalizedTextKey</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LocalizedText">
          <DataField>LocalizedText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
