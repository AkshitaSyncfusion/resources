  <DataSets>
    <DataSet Name="EmployeeDetailsDataset">
      <Query>
        <DataSourceName>AdventureWorks</DataSourceName>
        <CommandText>SELECT E.EmployeeID, E.Title,  E.ContactID, E.VacationHours, E.SickLeaveHours  
FROM HumanResources.Department D INNER JOIN HumanResources.EmployeeDepartmentHistory EDH ON D.DepartmentID = EDH.DepartmentID INNER JOIN HumanResources.Employee E ON EDH.EmployeeID = E.EmployeeID  
Where D.Name = @DepartmentName and D.GroupName = @DepartmentGroupName</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DepartmentName">
            <Value>=Parameters!DepartmentName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DepartmentGroupName">
            <Value>=Parameters!DepartmentGroupName.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="EmployeeID">
          <DataField>EmployeeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ContactID">
          <DataField>ContactID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="VacationHours">
          <DataField>VacationHours</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="SickLeaveHours">
          <DataField>SickLeaveHours</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
