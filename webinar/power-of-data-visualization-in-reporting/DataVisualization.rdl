    <DataSet Name="TopShows">
      <Query>
        <DataSourceName>ReportSampleDatabase</DataSourceName>
        <CommandText>SELECT 
[dbo].[Theater].[ShowName],
Sum([dbo].[Theater].[TicketsSold]) as TotalTicketsSold FROM [dbo].[Theater] Group by [dbo].[Theater].[ShowName]</CommandText>
      </Query>
      <Fields>
        <Field Name="ShowName">
