      <Query>
        <DataSourceName>ReportSampleDatabase</DataSourceName>
        <CommandText>SELECT [dbo].[Theater].[ShowDate],
[dbo].[Theater].[ShowStartDate],
[dbo].[Theater].[ShowName],
[dbo].[Theater].[BoxName],
Sum([dbo].[Theater].[TicketsSold]) as TicketsSold,
Sum([dbo].[Theater].[UnitPrice]) as UnitPrice FROM [dbo].[Theater] Group by [dbo].[Theater].[ShowName],[dbo].[Theater].[BoxName],[dbo].[Theater].[ShowDate],[dbo].[Theater].[ShowStartDate]</CommandText>
      </Query>
      <Fields>
